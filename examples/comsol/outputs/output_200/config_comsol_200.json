{
  "network": "./config_network.json",
  "run": {
    "tstop": 50.0,
    "dt": 0.025,
    "dL": 20.0,
    "spike_threshold": -15,
    "nsteps_block": 400
  },
  "target_simulator": "NEURON",
  "conditions": {
    "celsius": 34.0,
    "v_init": -80
  },
  "inputs": {
    "Extracellular_Stim": {
      "input_type": "lfp",
      "node_set": "all",
      "module": "comsol",
      "comsol_file": "./../bio_components/stimulations/COMSOL_200.txt"
    }
  },
  "output": {
    "log_file": "./outputs/output_200\\log.txt",
    "output_dir": "./outputs/output_200",
    "spikes_file": "./outputs/output_200\\spikes.h5",
    "spikes_file_csv": "./outputs/output_200\\spikes.csv",
    "spikes_sort_order": "time"
  },
  "components": {
    "morphologies_dir": "./../bio_components/morphologies",
    "synaptic_models_dir": "./../bio_components/synaptic_models",
    "mechanisms_dir": "./../bio_components/mechanisms",
    "biophysical_neuron_models_dir": "./../bio_components/biophysical_neuron_templates"
  },
  "node_sets": {
    "biophys_cells": {
      "model_type": "biophysical"
    }
  },
  "reports": {
    "calcium_concentration": {
      "cells": "biophys_cells",
      "variable_name": "cai",
      "module": "membrane_report",
      "file_name": "cai_report.h5",
      "sections": "soma",
      "enabled": true
    },
    "membrane_potential": {
      "cells": "biophys_cells",
      "variable_name": "v",
      "module": "membrane_report",
      "file_name": "v_report.h5",
      "sections": "soma",
      "enabled": true
    },
    "extracellular": {
      "cells": "biophys_cells",
      "variable_name": "e_extracellular",
      "module": "membrane_report",
      "file_name": "e_report.h5",
      "sections": "soma",
      "enabled": true
    },
    "im": {
      "cells": "biophys_cells",
      "variable_name": "i_membrane",
      "module": "membrane_report",
      "file_name": "i_report.h5",
      "sections": "soma",
      "enabled": true
    },
    "vext": {
      "cells": "biophys_cells",
      "variable_name": "vext",
      "module": "membrane_report",
      "file_name": "vext_report.h5",
      "sections": "soma",
      "transform": "first_element",
      "enabled": true
    },
    "ecp": {
      "cells": "biophys_cells",
      "variable_name": "v",
      "module": "extracellular",
      "electrode_positions": "./../bio_components/recXelectrodes/linear_electrode.csv",
      "file_name": "ecp.h5",
      "electrode_channels": "soma",
      "contributions_dir": "ecp_contributions",
      "enabled": true
    }
  },
  "config_path": "C:\\Users\\nilsv\\bmtk\\examples\\comsol\\config_comsol_200.json",
  "config_dir": "C:\\Users\\nilsv\\bmtk\\examples\\comsol",
  "networks": {
    "nodes": [
      {
        "nodes_file": "./network/column_nodes.h5",
        "node_types_file": "./network/column_node_types.csv"
      }
    ],
    "edges": [
      {
        "edges_file": "./network/column_column_edges.h5",
        "edge_types_file": "./network/column_column_edge_types.csv"
      }
    ]
  }
}